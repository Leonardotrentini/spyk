{
  "version": 2,
  "buildCommand": "cd googlestudio && npm install && npm run build",
  "outputDirectory": "googlestudio/dist",
  "devCommand": "cd googlestudio && npm run dev",
  "installCommand": "cd googlestudio && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
